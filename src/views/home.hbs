<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Assignments</title>
  </head>
  <body>
    <h1>Assignments</h1>
    <table>
      <tr>
        <th>Assignment</th>
        <th>Score</th>
        <th>Total</th>
        <th>Completed</th>
        <th>Actions</th>
      </tr>
      {{#each labs}}
        <tr>
          <td>{{this.name}}</td>
          <td>{{this.score}}</td>
          <td>{{this.total}}</td>
          <td>{{this.completed}}</td>
          <td><a href="">Delete</a></td>
        </tr>
      {{/each}}
    </table>
    <br />
    <h2>Overall Average: {{average}}</h2>
    <a href="/add">Add on Assignment</a>
  </body>
</html>